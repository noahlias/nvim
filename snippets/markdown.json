{
  "markdown_title ": {
    "prefix": ["title"],
    "body": [
      "------",
      "Title: ${1:title}",
      "Date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}",
      "Tags: [${3:tags}]",
      "Categories: [${4:categories}]",
      "------"
    ],
    "description": "Markdown title"
  },
  "Github Detail Expand": {
    "prefix": ["gt_detail"],
    "body": [
      "<details open>",
      "<summary>${1:summary}</summary>",
      "${0}",
      "</details>"
    ]
  }
}
