{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME/lua",
      "$VIMRUNTIME/lua/vim",
      "$VIMRUNTIME/lua/vim/lsp",
      "$LAZYROOT/lazy.nvim/lua/lazy"
    ]
  },
  "diagnostics.disable": ["missing-fields"]
}
